{
  "name": "Nurse Me",
  "description": "Video nurse hotline",
  "minimum_chrome_version": "24.0.1307.0",
  "version": "0.0.0.22",
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
  	"background": {
      "scripts": ["js/background.js"]
    }
  },
  "permissions": [
    "webview",
    "audioCapture", 
    "videoCapture",
  	"pushMessaging",
  	"notifications",
    "http://*/*"
  ]
}